target 'APOD' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for APOD
  pod 'Kingfisher'
  pod 'ObjectMapper'
  pod 'PureLayout'
  pod 'R.swift'
  pod 'RxAlamofire'
  pod 'RxCocoa'
  pod 'RxSwift'
  
  target 'APODTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'APODUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
